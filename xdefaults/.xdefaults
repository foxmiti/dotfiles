URxvt.letterSpace: 0
*.font: xft:Operator Mono Medium:pixelsize=13:antialias=true:hinting=true:hintstyle=hintslight,\
	    xft:symbola:size=9,\
	    -*-unifont-*-*-*-*-*-*-*-*-*-*-*-*,\
      xft:Wuncon Siji 10:size=10
	

Xft*dpi: 99
Xft*hinting: full
Xft*antialias: true
Xft*rgba: rgb

